{
  "name": "react-erd",
  "version": "1.0.10",
  "author": "CNimmo16",
  "keywords": [
    "Entity Relationship Diagram",
    "React",
    "ERD",
    "Database",
    "Database Diagram"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/CNimmo16/react-erd.git"
  },
  "license": "MIT",
  "main": "dist/RelationshipDiagram.cjs",
  "module": "dist/RelationshipDiagram.js",
  "types": "dist/RelationshipDiagram.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "check:lint": "eslint ./src/**/*.ts*",
    "check:lint:fix": "eslint ./src/**/*.ts* --fix",
    "check:style": "prettier --check .",
    "check:style:fix": "prettier --write .",
    "storybook": "concurrently 'npm:watch:styles' 'start-storybook -p 6006'",
    "build-storybook": "build-storybook",
    "build": "rimraf ./dist && concurrently 'npm:build:*'",
    "build:ts": "tsup ./src/**/*.{ts,tsx} --dts --sourcemap --format esm,cjs",
    "build:styles": "sass --load-path=node_modules/reactflow src/RelationshipDiagram.scss dist/style.css",
    "watch:styles": "yarn build:styles --watch",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@storybook/addon-actions": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/builder-webpack5": "^8.6.12",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^8.6.12",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "babel-loader": "^8.3.0",
    "babel-plugin-const-enum": "^1.2.0",
    "babel-plugin-transform-async-to-promises": "^0.8.18",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "d3-scale-chromatic": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.0",
    "prettier": "2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.4.0",
    "storybook": "^8.6.12",
    "style-loader": "^3.3.1",
    "tsup": "^8.4.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "@mdi/js": "^7.1.96",
    "@mdi/react": "^1.6.1",
    "reactflow": "^11.5.6",
    "sass": "^1.58.3"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  }
}
